<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">

<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">
    <class name="bm.app.model.ExerciseModel" table="tracker">
    <meta attribute="class-description">An exemplary class of ExerciseModel.</meta>
        <id name="id" type="java.lang.Integer" column="id"/>
        <property name="day" type="java.util.Date" column="day"/>
        <property name="exercisetype" type="java.lang.String" column="exercisetype"/>
        <property name="series" type="java.lang.Integer" column="series"/>
        <property name="repetitions" type="java.lang.Integer" column="repetitions"/>
    </class>
</hibernate-mapping>
